#: lib/osci/messages.py:129
#, python-format
msgid ""
"Combat with: %(4)s. HP lost: we %(1)d, they %(2)d.\n"
"\n"
"Enemy lost %(2)d HP, we lost %(1)d HP and %(3)d ships/structures. We attacked/were attacked by %(4)s."
msgstr ""
"Schlacht mit: %(4)s. TP verloren : Wir %(1)d, Sie %(2)d.\n"
"\n"
"Feind verlor %(2)d TP, Wir verloren %(1)d TP und %(3)d Schiffe/Strukturen. Wir attackierten/wurden angegriffen von %(4)s."

#: lib/osci/messages.py:130
#, python-format
msgid "Battle lost: we were defeated by %(1)s."
